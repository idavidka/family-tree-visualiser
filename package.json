  "scripts": {
    "colors": "node node/colors.js",
    "copy-to-cloud": "cp ./src/colors.ts ./functions/src/colors.ts && cp -R ./src/utils ./functions/src && cp -R ./src/classes ./functions/src && cp -R ./src/constants ./functions/src && cp -R ./src/types ./functions/src",
    "commit-revision": "git add ./src/configs/release.json && git commit -m \"Updated to revision $VERSION\" && git push",
    "add-revision": "VERSION=`node -pe 'JSON.parse(process.argv[1]).version' \"$(cat ./src/configs/release.json)\"`",
    "revision": "CI=`git rev-parse --short HEAD` && URL=`git config --get remote.origin.url` && GIT_DATE=`git show -s --format=%ci $CI` && DATE=$(date \"+%Y-%m-%d %H:%M:%S\") && echo \"{\\\"version\\\": \\\"$CI\\\", \\\"date\\\":\\\"$DATE\\\", \\\"url\\\":\\\"$URL\\\"}\" > ./src/configs/release.json",
    "deploy": "npm version patch && npm run revision && npm run build && firebase deploy && npm run add-revision && git add . && git commit -m \"Deployed version\" && git push",
    "start": "HOST=0.0.0.0 PORT=5555 react-app-rewired start",
    "build": "(npm run format || true) && GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "(npm run format || true) && CI=true react-app-rewired test --colors",
    "test:dev": "CI=true react-app-rewired test --colors",
    "test:watch": "npm run test -- --watch",
    "eject": "react-app-rewired eject",
    "format-main": "prettier --write src/**/*.ts{,x}",
    "format-sub": " prettier --write src/**/**/*.ts{,x}",
    "format": "npm run format-main || npm run format-sub",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "cloud:dev": "npm run serve --prefix ./functions"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/hsl-to-hex": "^1.0.0",
    "@types/lodash": "^4.14.197",
